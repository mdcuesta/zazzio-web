{
  "name": "zazzio",
  "version": "0.0.0",
  "author": "Michael Dela Cuesta <michael.dcuesta@gmail.com>",
  "private": true,
  "scripts": {
    "react-compile": "webpack --watch --config webpack.development.js",
    "start": "npm run react-compile | nodemon --exec babel-node ./bin/www --presets es2015",
    "test": "jest --verbose",
    "es-lint": "eslint --fix --config .eslintrc.json --output-file eslint-output.html --ext .js --ext .jsx *.js **/**",
    "sass-lint": "sass-lint --verbose --output-file sass-lint-output.html sass",
    "lint": "npm run es-lint && npm run sass-lint",
    "build-release": "webpack -p -progress --colors --config webpack.release.js",
    "release": "node zazzio.compiled.js"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "aws-sdk": "^2.5.2",
    "babel-runtime": "^6.11.6",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "bootstrap": "^4.0.0-alpha.4",
    "compression": "^1.6.2",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "csurf": "^1.9.0",
    "debug": "^2.2.0",
    "enzyme": "^2.4.1",
    "express": "^4.14.0",
    "express-react-views": "^0.10.2",
    "express-session": "^1.14.1",
    "express-validation": "^1.0.0",
    "flux": "^2.1.1",
    "font-awesome": "^4.6.3",
    "helmet": "^2.1.3",
    "joi": "^9.0.4",
    "jquery.2": "^1.0.0",
    "kickbox": "^2.0.1",
    "mongodb": "^2.2.5",
    "mongodb-migrate": "^2.0.1",
    "mongoose": "^4.6.0",
    "mongoose-auto-increment": "^5.0.1",
    "morgan": "^1.7.0",
    "motion-ui": "^1.2.2",
    "nexmo": "^1.0.0",
    "node-sass-middleware": "^0.9.8",
    "octicons": "^4.4.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "raven": "^0.12.1",
    "raven-js": "^3.7.0",
    "raygun": "^0.8.5",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "sendgrid": "^4.0.1",
    "serve-favicon": "^2.3.0",
    "tether": "^1.3.7",
    "validator": "^5.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-jest": "^14.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "envify": "^3.4.1",
    "eslint": "^3.4.0",
    "eslint-cli": "^1.1.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.2.0",
    "glob": "^7.0.5",
    "jest-cli": "^14.1.0",
    "jshint": "^2.9.2",
    "json-loader": "^0.5.4",
    "node-loader": "^0.5.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.0",
    "react-test-renderer": "^15.3.1",
    "sass-lint": "^1.9.1",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-node-externals": "^1.3.3"
  },
  "jest": {
    "rootDir": ".",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/core-js",
      "<rootDir>/node_modules/enzyme",
      "<rootDir>/node_modules/sinon",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/express",
      "<rootDir>/node_modules/passport"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ]
  }
