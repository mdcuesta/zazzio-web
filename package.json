{
  "name": "zazzio",
  "version": "0.0.0",
  "author": "Michael Dela Cuesta <michael.dcuesta@gmail.com>",
  "private": true,
  "scripts": {
    "react-compile": "webpack --watch --config webpack.development.js",
    "start": "npm run react-compile | nodemon --exec babel-node ./bin/www --presets es2015",
    "test": "jest --verbose",
    "build-release": "webpack --progress --colors --config webpack.release.js",
    "release": "node zazzio.compiled.js"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "bootstrap-sass": "^3.3.7",
    "cookie-parser": "^1.4.3",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "express-react-views": "^0.10.2",
    "mongo-migrate": "^0.1.0",
    "mongodb": "^2.2.5",
    "mongodb-migrate": "^2.0.1",
    "morgan": "^1.7.0",
    "node-sass-middleware": "^0.9.8",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-jest": "^14.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "envify": "^3.4.1",
    "glob": "^7.0.5",
    "jest-cli": "^14.1.0",
    "jshint": "^2.9.2",
    "json-loader": "^0.5.4",
    "node-loader": "^0.5.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-node-externals": "^1.3.3"
  },
  "jest": {
    "rootDir": ".",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/express"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  }
}
